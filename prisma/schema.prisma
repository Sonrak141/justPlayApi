// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Game {
  id               Int      @id @default(autoincrement())
  location        String
  date            DateTime
  teamLocal    String
  teamAway String
  image           String?
  type Int
}


